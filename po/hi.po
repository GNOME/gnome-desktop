msgstr "%a %-e %b_%l:%M:%S %p"

# Adapted for Hindi locale.
#: libgnome-desktop/gnome-wall-clock.c:385
msgid "%a %b %-e_%l:%M %p"
msgstr "%a %-e %b_%l:%M %p"

